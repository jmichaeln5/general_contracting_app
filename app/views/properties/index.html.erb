
<h1>All Properties</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.full_street_address %></td>
        <td><%= link_to 'View Property', property %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Property', new_property_path %>

<%= link_to 'New Property', new_user_property_path(@user) %>
